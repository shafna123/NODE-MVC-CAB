<h1 class="text-center"></h1>
<div class="container">
    <a href="/cabdetails/create" class="btn btn-primary">+ BOOK NOW </a>
    <table class="table table-striped mt-5">
       
        <tr>
           {{!-- <th>Booking ID</th> --}}
            <th>Cab ID</th>
            <th>Cab Model</th>
            <th>Cab Number</th>
            <th>Cab Capacity</th>
            <th>Driver ID</th>
           
          
        </tr>
        {{#each data}}
      
            <td>{{this.dataValues.cab_id}}</td>
            <td>{{this.dataValues.cab_model}}</td>
            <td>{{this.dataValues.cab_number}}</td>
            <td>{{this.dataValues.cab_capacity}}</td>
            <td>{{this.dataValues.driver_id}}</td>
            <td><a href="/booking/create/{{this.dataValues.cab_id}}">Book Now</a></td>
                  
            {{!-- <td>
                <a href="/cabdetails/update/{{this.dataValues.cab_id}}">Edit</a> |
                <a href="/cabdetails/delete/{{this.dataValues.cab_id}}" class="text-danger" onclick="return confirm('Are you sure you want to delete this')">Delete</a>
            </td> --}}
        </tr>
        {{/each}}
    </table>
     {{#if cab.role}}
        {{!-- <button class="btn btn-success">Add Cab</button> --}}
