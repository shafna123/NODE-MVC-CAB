


<h1 class="text-center"></h1>
<div class="container">
    {{!-- <a href="/driver/create" class="btn btn-primary">+ NEW DRIVER </a> --}}
    <table class="table table-striped mt-4">
        <tr>
           {{!-- <th>Booking ID</th> --}}
            <th>Driver ID</th>
            <th>Driver Name</th>
            <th>Driver Mobile Number</th>
            <th>Gender</th>
             <th>Driver Email</th>
              <th>Driver Paasword</th>
          
          
        </tr>
        {{#each data}}
        {{!-- <tr><td>{{this.dataValues.id}}</td> --}}
        {{!-- <td>{{this.dataValues.booking_id}}</td> --}}
            <td>{{this.dataValues.driver_id}}</td>
            <td>{{this.dataValues.driver_name}}</td>
            <td>{{this.dataValues.driver_mobile_number}}</td>
            <td>{{this.dataValues.gender}}</td>
            <td>{{this.dataValues.driver_email}}</td>
             <td>{{this.dataValues.driver_password}}</td>
              
            <td>
                <a href="/driver/update/{{this.dataValues.driver_id}}">Edit</a> |
                <a href="/driver/delete/{{this.dataValues.driver_id}}" class="text-danger" onclick="return confirm('Are you sure you want to delete this')">Delete</a>
            </td>
        </tr>
        {{/each}}
    </table>
     {{#if role}}
        {{!-- <button class="btn btn-success">Add Cab</button> --}}
             <a href="/driver/create" class="btn btn-success"> ADD DRIVER </a>
        {{/if}}
</div>