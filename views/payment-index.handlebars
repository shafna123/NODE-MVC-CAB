<h1 class="text-center"></h1>
<div class="container">
    <a href="/payment/create" class="btn btn-primary">+ NEW PAYMENT </a>
    <table class="table table-striped mt-4">
        <tr>
           {{!-- <th>Booking ID</th> --}}
            <th>Pick Up Location</th>
            <th>Drop Off Location</th>
            <th>Cost</th>
            
          
          
        </tr>
        {{#each data}}
        {{!-- <tr><td>{{this.dataValues.id}}</td> --}}
        {{!-- <td>{{this.dataValues.booking_id}}</td> --}}
            <td>{{this.dataValues.pick_up_location}}</td>
            <td>{{this.dataValues.drop_off_location}}</td>
            <td>{{this.dataValues.cost}}</td>
            
           
              
            <td>
                <a href="/payment/update/{{this.dataValues.id}}">Edit</a> |
                <a href="/payment/delete/{{this.dataValues.id}}" class="text-danger" onclick="return confirm('Are you sure you want to delete this')">Delete</a>
            </td>
        </tr>
        {{/each}}
    </table>
</div>